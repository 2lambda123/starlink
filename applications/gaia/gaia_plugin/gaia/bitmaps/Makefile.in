INSTALL      = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
BINDIR       = @BINDIR@
RM           = rm -f
SHELL        = /bin/sh
FILES        = *.xbm
srcdir	     = @srcdir@
TARGET       = ../src/gaia_bitmaps.C

all: $(TARGET)

All: 
	cd ..; $(MAKE)

# generate a C file that creates all of the bitmap files
# in this directory and installs them for Tk access by basename
$(TARGET): $(FILES)
	itclsh2.2 bitmaps.tcl > $(TARGET)

install:

clean:
	$(RM) *\~ "#"* core

depend:

distclean: clean
	rm -f Makefile	

man:

