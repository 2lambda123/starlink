interface autocrop

   parameter IN
      position 1
      type   'NDF'
      access 'READ'
      vpath  'PROMPT'
      ppath  'CURRENT'
      prompt 'Image to be cropped'
   endparameter

   parameter OUT
      position 2
      type   'NDF'
      access 'READ'
      vpath  'PROMPT'
      ppath  'CURRENT'
      prompt 'Output image'
   endparameter

endinterface
