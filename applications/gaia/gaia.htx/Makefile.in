# ------------------------------------------------------------------------
# 	Makefile for GAIA help library files
# ------------------------------------------------------------------------

INSTALL      = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
GAIA_LIBRARY = @GAIA_LIBRARY@
GAIA_HELP = $(GAIA_LIBRARY)/gaia.htx
RM           = rm -f
SHELL        = /bin/sh
FILES        = \
aperture.html domain.html optimal.html refine.html ardusage.html \
esp.html patch.html regions.html astrometry.html espusage.html \
patchusage.html statistics.html blink.html extract.html photom.html \
system.html catalogues.html extractor.html polarimetry.html \
transfer.html compare.html graphics.html polarusage.html \
transform.html contour.html grid.html positions.html view.html \
copy.html importtext.html printing.html xyprofile.html define.html \
index.html query.html xyprofileusage.html display.html inspect.html \
reference.html startupoptions.html \
apphotom.gif display.gif gridps.gif pixeltable.gif refine.gif \
ard_buttons.gif ellipse.gif importtext.gif positions.gif regions.gif \
blink.gif esp.gif optphotom.gif print.gif rubberband.gif contour.gif \
extractor.gif panel.gif query.gif slice.gif contourui.gif gaialogo.gif \
patch.gif rampprint.gif transfer.gif dataservers.gif graphics.gif \
photom.gif rampps.gif transform.gif define.gif grid.gif pick.gif \
reference.gif xyprofile.gif

PROGRAMS     =

srcdir	     = @srcdir@

all:

install-top:
	test -d $(GAIA_HELP) || mkdir -p $(GAIA_HELP)

install-files: install-top
	@set -x; for i in $(FILES) ; do \
	    $(INSTALL_DATA) $$i $(GAIA_HELP) ; \
	done

install: install-files

clean:
	$(RM) *\~ "#"*

depend:

distclean: clean
	rm -f Makefile

man:

FORCE:

