#!/bin/sh

#  back to basics

xsd cxx-tree --root-element VOTABLE \
             --namespace-map http://www.ivoa.net/xml/VOTable/v1.1=votable_11 \
             --parser-regex '/VOTABLE/VOTABLE_read/' \
             --serializer-regex '/VOTABLE/VOTABLE_write/' \
             --generate-doxygen \
             VOTable1.1.xsd

#  name mangling version, not used
#xsd cxx-tree --root-element VOTABLE \
#             --namespace-map http://www.ivoa.net/xml/VOTable/v1.1=votable \
#             --generate-serialization \
#             --type-naming ucc \
#             --function-naming knr \
#             --generate-doxygen \
#             VOTable1.1.xsd
#

doxygen VOTable1.1.hxx  > doxygen.log  2>&1
