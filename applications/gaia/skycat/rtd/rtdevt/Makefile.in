# Component level Makefile

SUBDIRS = src include man


do_all do_install do_clean do_depend do_man:
	$(AT)for i in $(SUBDIRS); do (cd $$i; $(MAKE) $@) done

do_distclean: clean
	$(AT)$(RM) -f Makefile
	$(AT)for i in $(SUBDIRS); do (cd $$i; $(MAKE) $@) done

include ../makefile.mk

