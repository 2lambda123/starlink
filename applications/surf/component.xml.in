<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@" support="S">
  <version>@PACKAGE_VERSION@</version>
  <path>applications/surf</path>
  <description>SURF - SCUBA User Reduction Facility</description>
  <abstract><p>
SURF is a set of ADAM tasks necessary for reducing demodulated
Submillimetre Common-User Bolometer Array (SCUBA) data obtained from
the James Clerk Maxwell Telescope. The tasks allows one to completely
re-reduce your SCUBA data.
  </p></abstract>
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>Tim Jenness</name>
      <uname>timj</uname>
    </person>
    <person>
      <name>John Lightfoot</name>
      <uname>jfl</uname>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <!-- <notes><p></p></notes> -->
</component>
