         parameter x2 {
            type LITERAL
            access WRITE
            vpath DEFAULT
            default 0.0D0
            helpkey *
          }

         parameter y1 {
            type LITERAL
            access WRITE
            vpath DEFAULT
            default 0.0D0
            helpkey *
          }

