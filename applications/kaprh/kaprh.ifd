         parameter device {
            ptype DEVICE
            position 1
            vpath GLOBAL PROMPT
            ppath GLOBAL CURRENT
            association <-GLOBAL.IMAGE_DISPLAY
            prompt {Name of the image display}
            helpkey *
          }

      }

