package kaprh {
   version PKG_VERS
   prefix krh

   executable kaprh_mon {

      action ovclear {
         alias {ovcl(ear)}

         helplib {$KAPRH_HELP}

         parameter current {
            position 2
            type _LOGICAL
            vpath DEFAULT
            ppath CURRENT DEFAULT
            prompt {Clear just the current picture?}
            default FALSE
            helpkey *
          }

         parameter device {
            ptype DEVICE
            position 1
            vpath GLOBAL PROMPT
            ppath GLOBAL CURRENT
            association <-GLOBAL.IMAGE_OVERLAY
            prompt {Name of overlay device}
            helpkey *
          }

      }

      action ovset {
         alias {ovse(t)}

         helplib {$KAPRH_HELP}

         parameter device {
            position 1
            ptype DEVICE
            access READ
            vpath PROMPT
            prompt {Image-display overlay}
            helpkey *
            association ->GLOBAL.IMAGE_OVERLAY
          }

      }

      action idclear {
         alias {idcl(ear)}

         helplib {$KAPRH_HELP}

         parameter current {
            position 2
            type _LOGICAL
            access READ
            vpath DEFAULT
            ppath CURRENT DEFAULT
            prompt {Clear just the current picture?}
            default FALSE
            helpkey *
