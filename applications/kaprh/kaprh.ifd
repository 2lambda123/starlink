            helpkey *
          }

         parameter frame {
            position 2
            type LITERAL
            vpath DEFAULT
            prompt {Co-ordinate Frame to be displayed}
            ppath DEFAULT
            default !
            helpkey *
          }

          parameter label {
            type LITERAL
            access WRITE
            vpath DEFAULT
            default 0.0D0
            helpkey *
          }

         parameter name {
            type LITERAL
            access WRITE
            vpath DEFAULT
            default 0.0D0
            helpkey *
          }

