  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>Sarah F Graves</name>
      <uname>sfg</uname>
      <email>s.graves@jach.hawaii.edu</email>
      <role>owner</role>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>2014</year>
    <name>Science and Technology Facilities Council</name>
  </copyright>
  <notes><p>Provides the 'starlink' LaTeX class</p></notes>
</component>
