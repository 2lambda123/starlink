      association '->GLOBAL.TVLOW'
   endparameter

   parameter XSTART
      type '_REAL'
      access 'WRITE'
      vpath 'DEFAULT'
      default 0.
      association '->GLOBAL.XSTART'
   endparameter

   parameter XEND
      type '_REAL'
      access 'WRITE'
      vpath 'DEFAULT'
      default 0.
      association '->GLOBAL.XEND'
   endparameter

   parameter HIGH
      type '_REAL'
      access 'WRITE'
      vpath 'DEFAULT'
      default 0.
      association '->GLOBAL.HIGH'
   endparameter

   parameter LOW
      type '_REAL'
      access 'WRITE'
