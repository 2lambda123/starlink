      default 0.
      association '<-GLOBAL.TVXEN'
   endparameter

   parameter TVLOW
      type '_REAL'
      access 'READ'
      vpath  'GLOBAL'
      ppath  'GLOBAL'
      default 0.
      association '<-GLOBAL.TVLOW'
   endparameter

   parameter TVHIGH
      type '_REAL'
      access 'READ'
      vpath  'GLOBAL'
      ppath  'GLOBAL'
      default 0.
      association '<-GLOBAL.TVHIGH'
   endparameter

   parameter TVFILE
      type '_CHAR'
