C+
C     H D S C O D E S . I N C
C
C     HDS Error codes.  This file is based on the one supplied as
C     part of ADAM, but since that is not actually part of HDS it
C     was felt that it needed to be included in the DTA_ package.
C
C                                            KS / AAO 11th March 1986
C     Modified:
C     
C     6th Oct 1987  KS/AAO  DAT__ACTIV added.
C     8th Jan 1992  KS/AAO  Completely redone for the new portable HDS version,
C                   using the decimal codes taken from dat_err.h in the new
C                   HDS code, rather than the hex constants used for the 
C                   original VAX version.
C+
      INTEGER  DAT__OK 

      INTEGER  DAT__LOCIN 
      INTEGER  DAT__TYPIN 
      INTEGER  DAT__NAMIN 
      INTEGER  DAT__MODIN 
      INTEGER  DAT__CONIN 
      INTEGER  DAT__DELIN 
      INTEGER  DAT__DIMIN 
      INTEGER  DAT__FILIN 
      INTEGER  DAT__OBJIN 
      INTEGER  DAT__GRPIN 
      INTEGER  DAT__SUBIN 
      INTEGER  DAT__COMEX 
      INTEGER  DAT__OBJNF 
      INTEGER  DAT__TRUNC 
      INTEGER  DAT__ACCON 
      INTEGER  DAT__CONER 
      INTEGER  DAT__UNSET 
      INTEGER  DAT__VERMM 
      INTEGER  DAT__PRMAP 
      INTEGER  DAT__RELIN 
      INTEGER  DAT__FILCK 
      INTEGER  DAT__FILNF 
      INTEGER  DAT__FILPR 
      INTEGER  DAT__INCHK 
      INTEGER  DAT__FATAL 
      INTEGER  DAT__WEIRD 

      INTEGER  DAT__EXCPA 
      INTEGER  DAT__UNKPA 
      INTEGER  DAT__ISOPN 
      INTEGER  DAT__ERACT 
      INTEGER  DAT__EREXH 
      INTEGER  DAT__NOMAP 
      INTEGER  DAT__ISMAP 
      INTEGER  DAT__STKOF 
      INTEGER  DAT__BOUND 

      INTEGER  DAT__ACTIV 

      INTEGER  DAT__FILCL 
      INTEGER  DAT__FILCR 
      INTEGER  DAT__FILMP 
      INTEGER  DAT__FILND 
      INTEGER  DAT__FILNX 
      INTEGER  DAT__FILRD 
      INTEGER  DAT__FILWR 
      INTEGER  DAT__NOMEM 

      PARAMETER (DAT__OK = 0)             ! OK (no error)

      PARAMETER ( DAT__LOCIN = 147358499) ! Locator invalid
      PARAMETER ( DAT__TYPIN = 147358507) ! Type invalid
      PARAMETER ( DAT__NAMIN = 147358515) ! Name invalid
      PARAMETER ( DAT__MODIN = 147358523) ! Mode invalid
      PARAMETER ( DAT__CONIN = 147358531) ! Conversion invalid
      PARAMETER ( DAT__DELIN = 147358539) ! Deletion invalid
      PARAMETER ( DAT__DIMIN = 147358547) ! Dimensions invalid
      PARAMETER ( DAT__FILIN = 147358555) ! File invalid
      PARAMETER ( DAT__OBJIN = 147358563) ! Object invalid
      PARAMETER ( DAT__GRPIN = 147358571) ! Group invalid
      PARAMETER ( DAT__SUBIN = 147358579) ! Subscripts invalid
      PARAMETER ( DAT__COMEX = 147358587) ! Component already exists
      PARAMETER ( DAT__OBJNF = 147358595) ! Object not found
      PARAMETER ( DAT__TRUNC = 147358603) ! Text truncated
      PARAMETER ( DAT__ACCON = 147358611) ! Access conflict
      PARAMETER ( DAT__CONER = 147358619) ! Conversion error
      PARAMETER ( DAT__UNSET = 147358627) ! Primitive data undefined
      PARAMETER ( DAT__VERMM = 147358635) ! Version mismatch
      PARAMETER ( DAT__PRMAP = 147358643) ! Primitive data mapped
      PARAMETER ( DAT__RELIN = 147358651) ! Relationship invalid
      PARAMETER ( DAT__FILCK = 147358659) ! File locking error
      PARAMETER ( DAT__FILNF = 147358667) ! File not found
      PARAMETER ( DAT__FILPR = 147358675) ! File protected
      PARAMETER ( DAT__INCHK = 147358683) ! Integrity check
      PARAMETER ( DAT__FATAL = 147358691) ! Fatal internal error
      PARAMETER ( DAT__WEIRD = 147358699) ! Unkown error

      PARAMETER ( DAT__EXCPA = 147358707) ! Too many data parameters
      PARAMETER ( DAT__UNKPA = 147358715) ! Unknown data parameter
      PARAMETER ( DAT__ISOPN = 147358723) ! Parameter is currently active
      PARAMETER ( DAT__ERACT = 147358731) ! Error activating DAT
      PARAMETER ( DAT__EREXH = 147358739) ! Error establishing DAT exit handler
      PARAMETER ( DAT__NOMAP = 147358747) ! Not mapped (JRG addition)
      PARAMETER ( DAT__ISMAP = 147358755) ! Data currently mapped
      PARAMETER ( DAT__STKOF = 147358763) ! Recursion stack overflow
      PARAMETER ( DAT__BOUND = 147358771) ! Outside bounds of object

      PARAMETER ( DAT__ACTIV = 147358779) ! HDS is already active

      PARAMETER ( DAT__FILCL = 147358787) ! File close error
      PARAMETER ( DAT__FILCR = 147358795) ! File create error
      PARAMETER ( DAT__FILMP = 147358803) ! File mapping error
      PARAMETER ( DAT__FILND = 147358811) ! File not deleted
      PARAMETER ( DAT__FILNX = 147358819) ! File not extended
      PARAMETER ( DAT__FILRD = 147358827) ! File read error
      PARAMETER ( DAT__FILWR = 147358835) ! File write error
      PARAMETER ( DAT__NOMEM = 147358843) ! Insufficient memory available
