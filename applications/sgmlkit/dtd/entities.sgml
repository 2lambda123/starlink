<!--
  These parameter entities are for use within this DTD only - they
  select which entity sets to use below.  These will be defined on the
  fly, probably by the document parser, to produce slightly different
  entity references depending on the target medium.
-->
<!entity % Only.plaintext "IGNORE">
<!entity % Only.LaTeX	"IGNORE">

<![ %Only.LaTeX [
  <!ENTITY copy   CDATA "\copyright{}"	-- copyright sign	-->
  <!ENTITY reg    CDATA "(R)"		-- registered sign	-->
  <!ENTITY amp    CDATA "\&"		-- ampersand		-->
  <!ENTITY gt     CDATA ">"		-- greater than		-->
  <!ENTITY lt     CDATA "<"		-- less than		-->
  <!ENTITY quot   CDATA "\dquote{}"	-- double quote		-->
  <!ENTITY nbsp   CDATA "~"		-- non breaking space	-->
  <!entity hash	  cdata "\#"		-- hash sign		-->
  <!entity pound  cdata "{\normalfont \pounds{}}" 
					-- pound sign		-->
  <!entity dollar cdata "\$"		-- dollar sign		-->
  <!entity ellip  cdata "\dots{}"	-- ellipses		-->
  <!entity emdash cdata "---"		-- em dash		-->
  <!entity endash cdata "--"		-- en dash		-->
  <!entity percent cdata "{\%}"		-- percent sign		-->
  <!entity deg    cdata "\ensuremath{{}^\circ}"
					-- degree - maths only!	-->
  <!entity times  cdata "\ensuremath{\times}"
					-- multiplication sign	-->
  <!entity latex  cdata "\LaTeX{}"	-- might as well...	-->
  <!entity pi     cdata "\ensuremath{\pi}" -- pi		-->
]]>

<![ %Only.plaintext [
  <!ENTITY amp    CDATA "&"	-- ampersand			-->
  <!ENTITY gt     CDATA ">"     -- greater than			-->
  <!ENTITY lt     CDATA "<"     -- less than			-->
  <!entity pound  cdata "GBP"	-- pound sign			-->
  <!entity nbsp   cdata " "	-- space			-->
  <!entity deg    cdata "deg"	-- degree			-->
  <!entity times  cdata "x"	-- multiplication sign		-->
]]>

<!-- else make them produce themselves (Add pound entity)	-->
<!entity copy   CDATA "(c)"	-- copyright sign		-->
<!entity reg    CDATA "(R)"	-- registered sign		-->
<!entity amp    CDATA "&amp;"   -- ampersand			-->
<!entity gt     CDATA "&gt;"    -- greater than			-->
<!entity lt     CDATA "&lt;"    -- less than			-->
<!entity quot   CDATA '"'	-- double quote			-->
<!entity nbsp   CDATA "&#38;#160;"  -- non breaking space	-->
<!entity hash	cdata "#"	-- hash sign			-->
<!entity pound  cdata "&#38;#163;" -- pound sign		-->
<!entity dollar cdata "$"	-- dollar sign			-->
<!entity ellip  cdata "..."	-- ellipses			-->
<!entity emdash cdata "--"	-- em dash 			-->
<!entity endash cdata "-"	-- en dash			-->
<!entity percent cdata "%"	-- percent sign			-->
<!entity deg    cdata "&#38;#176;"	-- degree		-->
<![ ignore [
<!entity times  cdata "&#38;#327;"	-- multiplication sign	-->
]]>
<!entity times  cdata "x"	-- multiplication sign		-->
<!entity latex  cdata "LaTeX"	-- might as well...		-->
<!entity pi     cdata " pi "	-- pi				-->


<!-- Character mnemonic entities -->

<!--
  Add different ISO Latin entities, depending on the target
  Put these entities in _here_, rather than before the special ones
  above. 
-->

<![ %Only.plaintext [
<!-- we're generating plain text - turn entities into codepoints -->
<!ENTITY % ISOlat1 PUBLIC
       "ISO 8879:1986//ENTITIES Added Latin 1//EN//HTML">
]]>
<![ %Only.LaTeX [
<!ENTITY % ISOlat1 PUBLIC
       "ISO 8879:1986//ENTITIES Added Latin 1//EN//LATEX">
]]>

<!-- These entities expand to &#34;#number; - ie, from mnemonic to -->
<!-- numeric entities -->
<!ENTITY % ISOlat1 PUBLIC
       "ISO 8879:1986//ENTITIES Added Latin 1//EN//ENT">

%ISOlat1;


