<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@" support="S">
  <version>@PACKAGE_VERSION@</version>
  <path>applications/kappa</path>
  <description>KAPPA - Kernel Application Package</description>
  <abstract><p>
KAPPA is an applications package comprising about 190
general-purpose commands for image processing, data visualisation, and
manipulation of the standard Starlink data format---the NDF.  It is
intended to work in conjunction with Starlink's various specialised
packages.
  </p></abstract>
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>David Berry</name>
      <uname>dsb</uname>
      <email>dsb@ast.man.ac.uk</email>
      <role>owner</role>
    </person>
    <person>
      <name>Malcolm Currie</name>
      <uname>mjc</uname>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <!-- <notes><p></p></notes> -->
</component>
