      <name>Patrick Wallace</name>
      <uname>ptw</uname>
      <email>ptw@star.rl.ac.uk</email>
      <role>owner</role>
    </person>
    <person>
      <name>Chris Clayton</name>
      <uname>cac</uname>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
