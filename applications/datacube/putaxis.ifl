interface PUTAXIS

   parameter IN
      position 1
      prompt      'Input NDF'
      type        'NDF'
      vpath       'PROMPT'
      ppath       'GLOBAL,CURRENT'
      association '<->GLOBAL.DATA_ARRAY'
      helpkey     '*'
   endparameter
   parameter SPECTRAL
      type _INTEGER
      vpath 'CURRENT,DEFAULT'
      prompt 'Spectral dispersion axis'
      ppath 'CURRENT,DYNAMIC,DEFAULT'
      default 3
      helpkey '*'
   endparameter
       
endinterface
