!  Parameter values used in IRG.
      INTEGER   IRG__SZMOD    ! Length of access mode strings.
      CHARACTER IRG__PREFX*6  ! String used as IRG group title prefix.
      CHARACTER IRG__NDFTP*4  ! NDF file type (.SDF or .sdf - machine
                              ! depenedent)

      PARAMETER (
     :            IRG__SZMOD  = 6,
     :            IRG__PREFX  = 'IRG:  ',
     :            IRG__NDFTP = '.sdf' )
* $Id$
