         helplib {$CCDPACK_HELP}

         parameter in {
                       # NDF to be modified
            position 1
            type NDF
            access READ
            vpath PROMPT
            ppath GLOBAL CURRENT
            prompt {Image data}
            helpkey *
          }

         parameter table {
                          # Keyword translation table
            position 2
            type FILENAME
            access READ
            vpath PROMPT
            ppath CURRENT
            prompt {FITS keyword translation table}
            helpkey *
          }

      }
