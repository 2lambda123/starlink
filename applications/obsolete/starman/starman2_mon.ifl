#
# IMKEY.IFL
#
interface IMKEY
   helplib   '$STARMAN_HELP_A IMKEY PARAMETERS'
   parameter IN
      vpath   global
      association <-global.imkey.in
      type    NDF
      access  READ
      helpkey 'IN'
      ppath   'dynamic,default'
      default !
      prompt 'Input image (null if none)?'
      position 1
   endparameter
   parameter OUT
      vpath   global
      association <-global.imkey.out
      type    NDF
      access  WRITE
      helpkey 'OUT'
      ppath   'dynamic,default'
      prompt 'Output image ?'
      default !
      position 2
   endparameter
   parameter SIZE
