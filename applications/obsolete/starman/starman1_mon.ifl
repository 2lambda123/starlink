   endparameter
   parameter SKY
      vpath   global
      association <-global.addstars.sky
      type    '_REAL'
      ppath   'dynamic'
      helpkey 'SKY'
      vpath   'global,prompt'
      association <->global.addstars.SKY
      prompt  'Sky level to add to image'
   endparameter
   parameter SKYPOIS
      vpath   global
      association <-global.addstars.skypois
      type    '_LOGICAL'
      ppath   'dynamic'
      helpkey 'SKYPOIS'
      vpath   'global,prompt'
      association <->global.addstars.SKYPOIS
      prompt  'Add extra Poisson noise for sky? (yes:no)'
   endparameter
   parameter SKYGAIN
      vpath   global
      association <-global.addstars.skygain
