   parameter PIXNOISE
      vpath   global
      association <-global.addstars.pixnoise
      type    '_REAL'
      ppath   'dynamic'
      helpkey 'PIXNOISE'
      vpath   'global,prompt'
      association <->global.addstars.PIXNOISE
      prompt  'Extra Poisson noise to add (factored by SKYGAIN)'
   endparameter
endinterface
