*+
*
*   CMISAF common blocks.
*
*-
      LOGICAL ISAFILE
      BYTE ISAFILENAME(81)
      BYTE ISARES ( 8 )
      BYTE ISANAP ( 8 )
      BYTE ISACAMERA ( 8 )
      BYTE ISANAME (40 )
      INTEGER ISADAY
      INTEGER ISATIME
      INTEGER ISASEQ
      INTEGER ISAMONTH
      INTEGER ISAYEAR
      REAL*8 ISATEMP

      COMMON / IUEFILE   / ISAFILE
      COMMON / IUEFILE_1 / ISAFILENAME, ISARES, ISANAP, ISACAMERA,
     :                     ISANAME
      COMMON / IUEFILE_2 / ISADAY, ISATIME, ISASEQ, ISAMONTH, ISAYEAR
      COMMON / IUEFILE_3 / ISATEMP
