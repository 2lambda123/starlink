      ENDPARAMETER
   ENDINTERFACE

   INTERFACE WASCII
      PARAMETER SPECTRUM
         POSITION  1
         TYPE 'LITERAL'       
         VPATH 'PROMPT'                        
         PPATH 'CURRENT,DYNAMIC'
         PROMPT '(SPectrum) File to be converted to ASCII file'
      ENDPARAMETER
      PARAMETER FILE
         POSITION  2
         TYPE 'LITERAL'       
         VPATH 'PROMPT'                        
         PPATH 'CURRENT,DYNAMIC'
         PROMPT '(FIle) Name of ASCII format file to be created'
      ENDPARAMETER
   ENDINTERFACE
ENDMONOLITH
