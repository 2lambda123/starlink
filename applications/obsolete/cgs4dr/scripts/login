# CGS4DR
setenv SYSTEM `uname -m`_`uname -s`
if ( -e /ukirt_sw/${SYSTEM}/bin/cgs4dr/cgs4dr ) then
   setenv CGS4DR_ROOT /ukirt_sw/${SYSTEM}/bin/cgs4dr
endif

# Exit the script with status=OK
exit 0
