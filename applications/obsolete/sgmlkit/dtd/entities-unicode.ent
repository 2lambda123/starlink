<!--
  Additional entities, for use with XML.  These are extracted from the
  XML versions of the ISO entities contained in dtd/ISOent/xml.

  &amp and &lt are predeclared in XML.

  $Id$
-->

<!--
  The entity sets which are used when parsing XML ought to be
  specified in terms of Unicode character points.  The HTML output
  appears not to cause problems, but it does causes problems for
  LaTeX, which reacts badly when it encounters \char65440 or the like.
  This problem is, I'm positive, superable in LaTeX, but messy.  This
  should probably be deferred until after the rewrite of the
  processing system.  At present, it's been `fixed' by defining the
  `unicode' entities as ordinary characters, commenting out the
  definitions of &amp &gt and &lt, and declaring percent to be &#37.
-->

<!ENTITY copy   	"(c)"	><!-- copyright sign	-->
<!--ENTITY amp    	"&#38;#38;"	--><!-- ampersand		-->
<!--ENTITY gt     	"&#62;"	--><!-- greater than	-->
<!--ENTITY lt     	"&#38;#60;"	--><!-- less than		-->
<!ENTITY quot   	'"'	><!-- double quote	-->
<!ENTITY nbsp   	" "	><!-- non breaking space-->
<!ENTITY hash		"#"	><!-- hash sign		-->
<!ENTITY pounds		"GBP"	><!-- pound sign	-->
<!ENTITY dollar 	"$"	><!-- dollar sign	-->
<!ENTITY ellip  	"..."	><!-- ellipses		-->
<!ENTITY emdash 	"---"	><!-- em dash 		-->
<!ENTITY endash 	"--"	><!-- en dash		-->
<!ENTITY percent	"&#37;"	><!-- percent sign	-->
<!ENTITY deg    	"^o"	><!-- degree		-->
<!ENTITY dot		"."	><!-- non-sentence-ending period -->
<!ENTITY tilde		"~"	><!-- tilde		-->
<!ENTITY underscore	"_"	><!-- underscore	-->
<!ENTITY backslash	"\"	><!-- backslash		-->
<!ENTITY verticalbar	"|"	><!-- vertical bar	-->
<!ENTITY latex  	"LaTeX"	><!-- might as well...	-->
<!ENTITY tex		"TeX"	><!-- might as well...	-->


<![IGNORE[
<!ENTITY gt		"&#x003E;"> <!-- GREATER-THAN SIGN -->
<!ENTITY pounds		"&#x00A3;"> <!-- POUND SIGN -->
<!ENTITY dollar		"&#x0024;"> <!-- DOLLAR SIGN -->
<!ENTITY copy		"&#x00A9;"> <!-- COPYRIGHT SIGN -->
<!ENTITY nbsp		"&#x00A0;"> <!-- NO-BREAK SPACE -->
<!ENTITY percent	"&#x0025;"> <!-- PERCENT SIGN -->
<!ENTITY deg		"&#x00B0;"> <!-- DEGREE SIGN -->
<!ENTITY emdash		"&#x2014;"> <!-- EM DASH -->
<!ENTITY endash		"&#x2013;"> <!-- EN DASH -->
<!ENTITY ellip		"&#x2026;"> <!-- HORIZONTAL ELLIPSIS -->
<!ENTITY tilde		"&#x007E;"> <!-- TILDE -->
<!ENTITY hash		"&#x0023;"> <!-- NUMBER SIGN -->
<!ENTITY quot		"&#x0022;"> <!-- QUOTATION MARK -->
<!ENTITY underscore	"&#x005F;"> <!-- underscore		-->
<!ENTITY backslash	"&#x005C;"> <!-- backslash -->
<!ENTITY verticalbar	"&#x007C;"> <!-- vertical bar -->

<!ENTITY latex  "LaTeX"	> <!-- might as well...	-->
<!ENTITY tex	"TeX"	> <!-- might as well...	-->
]]>
