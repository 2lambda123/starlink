LSE NEW PLACEHOLDER PARAMETER_TYPE TERMINAL IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER DUPLICATION CONTEXT_DEPENDENT PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER LEADING "type     " PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "Specify the data type of the parameter.  This can be any of the" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "HDS primitive data types (_INTEGER, _REAL, etc.), or it may be a" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "user-defined HDS type.  The type specified may be used by the" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "parameter system to check that the value supplied for the" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "parameter is of a type that the application is expecting.  A" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "type of UNIV may also be specified - this means that any type of" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "object is acceptable (i.e. no type checking should occur). If a" ADD PARAMETER_TYPE IFL
    LSE SET PLACEHOLDER TERMINAL LINE "type is not specified, then UNIV is assumed." ADD PARAMETER_TYPE IFL

LSE NEW PLACEHOLDER PROLOGUE NONTERMINAL IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF PROLOGUE IFL
    LSE SET PLACEHOLDER DUPLICATION CONTEXT_DEPENDENT PROLOGUE IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "  Name:" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "     {module_name}.IFL" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "  Type of Module:" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "     ADAM A-task parameter interface." EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "  Authors:" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "     {original_author}" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "  History:" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "     {original_version}" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL
    LSE SET PLACEHOLDER BODY LINE "" EXPAND 0 SPACE NEXT ADD PROLOGUE IFL

LSE NEW PLACEHOLDER PROMPT_SOURCE MENU IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER DUPLICATION HORIZONTAL PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER LEADING "ppath    '" PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER TRAILING "'" PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER SEPARATOR , PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Current" "" TOKEN OFF ADD PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Default" "" TOKEN OFF ADD PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Dynamic" "" TOKEN OFF ADD PROMPT_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Global" "" TOKEN OFF ADD PROMPT_SOURCE IFL

LSE NEW PLACEHOLDER PROMPT_STRING TERMINAL IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF PROMPT_STRING IFL
    LSE SET PLACEHOLDER DUPLICATION CONTEXT_DEPENDENT PROMPT_STRING IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON PROMPT_STRING IFL
    LSE SET PLACEHOLDER LEADING "prompt   '" PROMPT_STRING IFL
    LSE SET PLACEHOLDER TRAILING "'" PROMPT_STRING IFL
    LSE SET PLACEHOLDER TERMINAL LINE "An optional prompt string, which will be used when a user is asked to" ADD PROMPT_STRING IFL
    LSE SET PLACEHOLDER TERMINAL LINE "supply a value for the parameter.  If a prompt string is not given," ADD PROMPT_STRING IFL
    LSE SET PLACEHOLDER TERMINAL LINE "then the keyword will be used to construct a prompt instead." ADD PROMPT_STRING IFL

LSE NEW PLACEHOLDER UPPER_BOUND TERMINAL IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF UPPER_BOUND IFL
    LSE SET PLACEHOLDER DUPLICATION CONTEXT_DEPENDENT UPPER_BOUND IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON UPPER_BOUND IFL
    LSE SET PLACEHOLDER TERMINAL LINE "The upper bound to be placed on the parameter's value." ADD UPPER_BOUND IFL

LSE NEW PLACEHOLDER VALUE_SOURCE MENU IFL
    LSE SET PLACEHOLDER AUTO SUBSTITUTE OFF VALUE_SOURCE IFL
    LSE SET PLACEHOLDER DUPLICATION HORIZONTAL VALUE_SOURCE IFL
    LSE SET PLACEHOLDER PSEUDOCODE ON VALUE_SOURCE IFL
    LSE SET PLACEHOLDER LEADING "vpath    '" VALUE_SOURCE IFL
    LSE SET PLACEHOLDER TRAILING "'" VALUE_SOURCE IFL
    LSE SET PLACEHOLDER SEPARATOR , VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Current" "" TOKEN OFF ADD VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Default" "" TOKEN OFF ADD VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Dynamic" "" TOKEN OFF ADD VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Global" "" TOKEN OFF ADD VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Noprompt" "" TOKEN OFF ADD VALUE_SOURCE IFL
    LSE SET PLACEHOLDER MENU LINE "Prompt" "" TOKEN OFF ADD VALUE_SOURCE IFL

LSE NEW TOKEN -> TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "write only" -> IFL
    LSE SET TOKEN BODY LINE -> EXPAND 0 SPACE NEXT ADD -> IFL

LSE NEW TOKEN <- TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "read only" <- IFL
    LSE SET TOKEN BODY LINE <- EXPAND 0 SPACE NEXT ADD <- IFL

LSE NEW TOKEN <-> TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "read and write" <-> IFL
    LSE SET TOKEN BODY LINE <-> EXPAND 0 SPACE NEXT ADD <-> IFL

LSE NEW TOKEN CURRENT TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Use the current value of the parameter" CURRENT IFL
    LSE SET TOKEN BODY LINE CURRENT EXPAND 0 SPACE NEXT ADD CURRENT IFL

LSE NEW TOKEN DEFAULT TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Use the default value specified in the interface module" DEFAULT IFL
    LSE SET TOKEN BODY LINE DEFAULT EXPAND 0 SPACE NEXT ADD DEFAULT IFL

LSE NEW TOKEN DYNAMIC TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Use the dynamic (application-specified) parameter value" DYNAMIC IFL
    LSE SET TOKEN BODY LINE DYNAMIC EXPAND 0 SPACE NEXT ADD DYNAMIC IFL

LSE NEW TOKEN GLOBAL TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Use the value of the associated (global) parameter" GLOBAL IFL
    LSE SET TOKEN BODY LINE GLOBAL EXPAND 0 SPACE NEXT ADD GLOBAL IFL

LSE NEW TOKEN NOPROMPT TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Do not prompt the user for a value" NOPROMPT IFL
    LSE SET TOKEN BODY LINE NOPROMPT EXPAND 0 SPACE NEXT ADD NOPROMPT IFL

LSE NEW TOKEN PROMPT TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "Prompt the user and obtain a value" PROMPT IFL
    LSE SET TOKEN BODY LINE PROMPT EXPAND 0 SPACE NEXT ADD PROMPT IFL

LSE NEW TOKEN READ TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "read only" READ IFL
    LSE SET TOKEN BODY LINE READ EXPAND 0 SPACE NEXT ADD READ IFL

LSE NEW TOKEN UPDATE TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "read and write" UPDATE IFL
    LSE SET TOKEN BODY LINE UPDATE EXPAND 0 SPACE NEXT ADD UPDATE IFL

LSE NEW TOKEN WRITE TERMINAL IFL
    LSE SET TOKEN DESCRIPTION "write only" WRITE IFL
    LSE SET TOKEN BODY LINE WRITE EXPAND 0 SPACE NEXT ADD WRITE IFL
