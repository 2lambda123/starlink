    LSE SET PLACEHOLDER PSEUDOCODE ON ", {INTEGER_EXP}" STARLINK_FORTRAN
    LSE SET PLACEHOLDER BODY LINE ", {integer_exp}" EXPAND 0 SPACE NEXT ADD ", {INTEGER_EXP}" STARLINK_FORTRAN

