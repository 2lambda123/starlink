LSE NEW TOKEN WHILE TERMINAL STARLINK_FORTRAN
    LSE SET TOKEN DESCRIPTION "DO WHILE loop implemented in Standard Fortran 77" WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "{lab}" FIXED 2 SPACE NEXT ADD WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "CONTINUE ! Start of 'DO WHILE' loop" EXPAND 10 SPACE SAME ADD WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "	IF ( {logical_exp} ) THEN" EXPAND 0 SPACE NEXT ADD WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "		{executable_statement}..." EXPAND 0 SPACE NEXT ADD WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "		GO TO {lab}" EXPAND 0 SPACE NEXT ADD WHILE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "	END IF" EXPAND 0 SPACE NEXT ADD WHILE STARLINK_FORTRAN

LSE NEW TOKEN WORD TERMINAL STARLINK_FORTRAN
    LSE SET TOKEN DESCRIPTION "INTEGER * 2 data type" WORD STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "	INTEGER * 2" EXPAND 0 SPACE NEXT ADD WORD STARLINK_FORTRAN

LSE NEW TOKEN WRITE TERMINAL STARLINK_FORTRAN
    LSE SET TOKEN DESCRIPTION "Internal WRITE statement" WRITE STARLINK_FORTRAN
    LSE SET TOKEN HELP TOPIC "STATEMENT WRITE INTERNAL " WRITE STARLINK_FORTRAN
    LSE SET TOKEN BODY LINE "	WRITE ( {ifile}, {ifmt}, [iwrite_parameter]... ) [io_elm]..." EXPAND 0 SPACE NEXT ADD WRITE STARLINK_FORTRAN

