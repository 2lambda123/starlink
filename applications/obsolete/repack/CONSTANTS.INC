*+CONSTANTS Define any useful constant
	REAL		PI		! Circular constant
	REAL		PIBY2		! PI / 2
	REAL		TWOPI		! 2 * PI
	REAL		DTOR		! Degrees to rads
	REAL		RTOD		! Rads to degrees
	REAL		RTOAS		! Rads to arc secs
	DOUBLE PRECISION CTOE(3,3)	! Celestial to Ecl (2000 epoch + equi)
	DOUBLE PRECISION ETOC(3,3)	! Ecliptic to Cel  (2000 epoch + equi)
	DOUBLE PRECISION CTOG(3,3)	! Cel to gal (2000 equi)
	DOUBLE PRECISION GTOC(3,3) 	! Gal to Cel (2000 equi)

	PARAMETER 	( PI    = 3.141592654 )
	PARAMETER	( PIBY2 = 1.5707963 )
	PARAMETER	( TWOPI = 6.283185308 )
	PARAMETER	( DTOR  = 0.01745329252 )
	PARAMETER	( RTOD  = 57.295779 )
	PARAMETER	( RTOAS = 206264.8062 )

