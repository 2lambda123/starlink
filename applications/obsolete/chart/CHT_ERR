*  Global Constants:
      INTEGER CHT__UNOIF         ! Unable to open intermediate file
      PARAMETER ( CHT__UNOIF = 1 )

c      INTEGER CHT__NONUM         ! No number found
c      PARAMETER ( CHT__NONUM = 1 )

      INTEGER CHT__FMTERR        ! Invalid number format
      PARAMETER ( CHT__FMTERR = 2 )

      INTEGER CHT__IVSEX         ! CONVPOS - Invalid sexagesimal number
      PARAMETER ( CHT__IVSEX = 2 )

      INTEGER CHT__DECBG         ! CONVDEC - DEC too big
      PARAMETER ( CHT__DECBG = 3 )

      INTEGER CHT__RABIG         ! CONVRA - RA too big
      PARAMETER ( CHT__RABIG = 3 )

      INTEGER CHT__NOCAT         ! Can't open star catalogue
      PARAMETER ( CHT__NOCAT = 1001 )

      INTEGER CHT__NODEV         ! Can't open plotting device
      PARAMETER ( CHT__NODEV = 1002 )

      INTEGER CHT__NOFIL         ! Can't open file
      PARAMETER ( CHT__NOFIL = 1003 )

      INTEGER CHT__BADRD         ! Can't read file
      PARAMETER ( CHT__BADRD = 1004 )

C      [local_constant_specification]...
*.

