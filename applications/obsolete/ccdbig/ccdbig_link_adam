echo "`img_link_adam` `fio_link_adam` `trn_link_adam` \
      `chr_link_adam` `prm_link_adam` `psx_link_adam` \
      `ard_link_adam` `grp_link_adam` -lpda " \
    | awk 'BEGIN{RS=" ";FS="\n"}
                  {f[i++]=$1}
                  END{for(;i--;)if(!w[f[i]]++)l=f[i]" "l;print l}'
