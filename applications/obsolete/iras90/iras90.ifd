				 # available in IRA_CTOR. 
            type LITERAL
            vpath PROMPT { DYNAMIC} { CURRENT }
            ppath DYNAMIC CURRENT
            prompt {First coord of source pos. eg RA}
            helpkey *
          }

         parameter sourcecoord2 {
