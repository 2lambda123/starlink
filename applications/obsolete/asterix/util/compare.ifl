INTERFACE COMPARE

   PARAMETER DATA
      POSITION    1
      TYPE        'LITERAL'
      ASSOCIATION '<->GLOBAL.FITDAT'
      VPATH       'GLOBAL,PROMPT'
      PROMPT      'Data'
   ENDPARAMETER

   PARAMETER MODEL
      POSITION    2
      TYPE        'LITERAL'
      ASSOCIATION '<->GLOBAL.FITMOD'
      VPATH       'GLOBAL,PROMPT'
      PROMPT      'Model'
   ENDPARAMETER

   PARAMETER POISSON
      TYPE        '_LOGICAL'
      DEFAULT     Y
      PROMPT      'Poisson data errors (Y/N)'
      HELP        'Root n data errors to be assumed; otherwise adopt unit errors'
   ENDPARAMETER

ENDINTERFACE
