interface ICOLOUR
  parameter MODE
    position 1
    type '_CHAR'
    default 'HELP'
    prompt 'Mode'
    help 'Reply with HELP for a full list'
  endparameter
  parameter FILE
    position 2
    type '_CHAR'
    prompt 'Table'
    help 'File containing or to contain colour table'
  endparameter
endinterface
