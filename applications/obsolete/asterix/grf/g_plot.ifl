interface G_PLOT
  parameter KEY
    type '_LOGICAL'
    default N
    vpath 'DEFAULT'
    prompt 'Input from keyboard'
    help 'Otherwise input is via cursor where appropriate'
  endparameter
  parameter X
    position 1
    type '_REAL'
    prompt 'X position'
    help 'new x position to draw to'
  endparameter
  parameter Y
    position 2
    type '_REAL'
    prompt 'Y position'
    help 'new y position to draw to'
  endparameter
endinterface
