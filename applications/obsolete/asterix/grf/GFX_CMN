*+  GFX_CMN - global variables for interactive GRAFIX

      LOGICAL G_OPEN
      LOGICAL G_NDF1,G_NDF2,G_NDF3,G_1DSET,G_PRIM1,G_PRIM2
      LOGICAL G_ASCIIY,G_ASCIIXY,G_ASCIIXYZ
      LOGICAL G_CINDEX
      INTEGER      G_ID
      CHARACTER*80 G_XLABEL,G_YLABEL,G_TITLE

      COMMON /GFX_CMN/ G_OPEN,G_NDF1,G_NDF2,G_NDF3,
     :                 G_1DSET,G_PRIM1,G_PRIM2,G_ASCIIY,G_ASCIIXY,
     :                 G_ASCIIXYZ,G_CINDEX,G_ID
      COMMON /GFX_CMN_C/ G_XLABEL,G_YLABEL,G_TITLE
