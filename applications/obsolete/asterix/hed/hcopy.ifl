INTERFACE HCOPY

    PARAMETER INP
       TYPE 'UNIV'
       position 1
       prompt 'Object to copy'
       help 'Full specification of input object eg. file.next.object'
    ENDPARAMETER

    PARAMETER OUT
       TYPE '_CHAR'
       position 2
       prompt 'Object to copy to'
       help 'Full name of output object'
    ENDPARAMETER

ENDINTERFACE
