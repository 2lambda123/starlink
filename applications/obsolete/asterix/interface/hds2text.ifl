INTERFACE HDS2TEXT

 PARAMETER IN1
  TYPE 'LITERAL'
  POSITION 1
  PROMPT 'First input object'
 ENDPARAMETER

 PARAMETER IN2
  TYPE 'LITERAL'
  POSITION 2
  DEFAULT !
  PROMPT 'Second input object'
 ENDPARAMETER

 PARAMETER IN3
  TYPE 'LITERAL'
  POSITION 3
  DEFAULT !
  PROMPT 'Third input object'
 ENDPARAMETER

 PARAMETER IN4
  TYPE 'LITERAL'
  POSITION 4
  DEFAULT !
  PROMPT 'Fourth input object'
 ENDPARAMETER

 PARAMETER IN5
  TYPE 'LITERAL'
  POSITION 5
  DEFAULT !
  PROMPT 'Fifth input object'
 ENDPARAMETER

 PARAMETER IN6
  TYPE 'LITERAL'
  POSITION 6
  DEFAULT !
  PROMPT 'Sixth input object'
 ENDPARAMETER

 PARAMETER IN7
  TYPE 'LITERAL'
  POSITION 7
  DEFAULT !
  PROMPT 'Seventh input object'
 ENDPARAMETER

 PARAMETER IN8
  TYPE 'LITERAL'
  POSITION 8
  DEFAULT !
  PROMPT 'Eighth input object'
 ENDPARAMETER

 PARAMETER IN9
  TYPE 'LITERAL'
  POSITION 9
  DEFAULT !
  PROMPT 'Ninth input object'
 ENDPARAMETER

 PARAMETER FMT
  PROMPT 'Output format'
  VPATH 'PROMPT'
  PPATH 'DEFAULT'
  TYPE 'LITERAL'
  DEFAULT '1PG14.6'
  HELP 'Any floating point Fortran format, eg. F12.5, G11.4, 1PG14.6'
 ENDPARAMETER

 PARAMETER OUT
  PROMPT 'Name of output ASCII text file'
  TYPE '_CHAR'
  HELP 'Text file to be filled with data from HDS objects'
 ENDPARAMETER

ENDINTERFACE
