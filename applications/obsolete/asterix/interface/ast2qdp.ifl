interface AST2QDP

  parameter INP
    type 'UNIV'
    position 1
    ppath 'GLOBAL'
    association '<->GLOBAL.BINDS'
    prompt 'Dataset'
    help 'Name of 1D binned dataset'
  endparameter

  parameter OUT
    type 'LITERAL'
    position 2
    vpath 'CURRENT'
    ppath 'CURRENT'
    prompt 'Output QDP data file'
    help 'QDP file containing data and commands'
  endparameter

endinterface
