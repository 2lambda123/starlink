      DOUBLE PRECISION XINC   ! X-axis increment in arcseconds per pixel at
                              ! reference pixel
      DOUBLE PRECISION YINC   ! Y-axis increment in arcseconds per pixel at
                              ! reference pixel
      DOUBLE PRECISION IMROT  ! Rotation of axes (degrees)

      COMMON / PROJCMN / XINC, YINC, IMROT

