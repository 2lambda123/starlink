*  PROJ_ local status values.
      INTEGER PROJ__UNDEFINED   ! Status value for undefined projection
      PARAMETER ( PROJ__UNDEFINED = 1 )

      INTEGER PROJ__BADLM       ! Bad input direction cosines
      PARAMETER ( PROJ__BADLM = 2 )

      INTEGER PROJ__NOCONV      ! No convergence of iterative processes
      PARAMETER ( PROJ__NOCONV = 3 )

      INTEGER PROJ__NSCHPROJ    ! No such projection
      PARAMETER ( PROJ__NSCHPROJ = 4 )

*  Other constants.
      DOUBLE PRECISION PROJ__SMALL ! Small value
      PARAMETER ( PROJ__SMALL = 1.0D-05 )

*.
* $Id$
