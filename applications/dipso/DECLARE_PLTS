*
*   An include file used by DIPSO (see sun50)
*
*
*   Parameters defined the contents of the HTFAC array:
*
      INTEGER NHTFAC, MJTICK, MNTICK, NUMLAB, TXTLAB, MARKS, PWRITE

      PARAMETER( NHTFAC = 6 )  ! Size of HTFAC array

      PARAMETER( MJTICK = 1 )  ! Major tick marks index
      PARAMETER( MNTICK = 2 )  ! Minor tick marks index
      PARAMETER( NUMLAB = 3 )  ! Numeric axis labels index 
      PARAMETER( TXTLAB = 4 )  ! Textual axis labels index
      PARAMETER( MARKS  = 5 )  ! Markers index
      PARAMETER( PWRITE = 6 )  ! Text generated by the PWRITE command index

*
*   Data for PLTARR:
*
      LOGICAL COLOUR
      LOGICAL CROPIT
      LOGICAL ERASEBOX
      LOGICAL FILL
      LOGICAL FRZONE
      LOGICAL LABELFLAG
      LOGICAL LOGAXX
      LOGICAL LOGAXY
      LOGICAL NONUM
      LOGICAL REALSIZE
      LOGICAL TRIMX
      LOGICAL TRIMY
      LOGICAL ZONECLEAR( 0:100 )
      LOGICAL ZONEDEF( 0:100 )

      INTEGER DEVTYP
      INTEGER IEXPX
      INTEGER IEXPY
      INTEGER IFONT
      INTEGER IGRIDSTYLE
      INTEGER IPAL
      INTEGER IPREC
      INTEGER ITICKS( 2 )
      INTEGER LNTYPE
      INTEGER MARKSTYLE
      INTEGER NDEGREES
      INTEGER NSIDES
      INTEGER NZONEN1

      REAL DEFHEIGHT
      REAL GRID( 8 )
      REAL GRIDP( 5 )
      REAL GRIDS( 8, 0:100 )
      REAL HTFAC( NHTFAC )
      REAL MAJTICKS( 2 )
      REAL MINTICKS( 2 )
      REAL XGSIZE
      REAL XLIM1
      REAL XLIM2
      REAL LNWIDTH
      REAL XLWIDTH
      REAL XS2NDC
      REAL YGSIZE
      REAL YLIM1
      REAL YLIM2
      REAL YS2NDC

      CHARACTER * ( 2 ) GPOS

      COMMON /PLTARR1/ DEVTYP, NZONEN1, LNTYPE, IPREC, IFONT, IPAL, 
     :                 IGRIDSTYLE, NDEGREES, NSIDES, MARKSTYLE, ITICKS

      COMMON /PLTARR2/ XLWIDTH, HTFAC, DEFHEIGHT, XLIM1, XLIM2, 
     :                 YLIM1, YLIM2, XS2NDC, YS2NDC, XGSIZE, YGSIZE,
     :                 MINTICKS, MAJTICKS, GRID, GRIDP, GRIDS, LNWIDTH

      COMMON /PLTARR3/ FILL, COLOUR, LABELFLAG, TRIMX, TRIMY, ERASEBOX, 
     :                 LOGAXX, LOGAXY, REALSIZE, FRZONE, NONUM, ZONEDEF, 
     :                 ZONECLEAR, IEXPY, IEXPX, CROPIT

      COMMON /PLTARR4/ GPOS

      INTEGER IZBASE
      INTEGER IZW

      COMMON /ZONES/ IZBASE, IZW

      REAL DIVX
      REAL DIVY

      COMMON /DIVFACTOR/ DIVX, DIVY

      REAL YLIM( 2 )

      COMMON /QYMEM / YLIM

      INTEGER PLTCAL

      COMMON /PLTCAL/ PLTCAL

*
