*+
*
*    Include file for polarimetry reduction
*
*-

      INTEGER MAXCHANS,MAXCYCLES
      PARAMETER (MAXCHANS = 10)
      PARAMETER (MAXCYCLES = 1000)

      DOUBLE PRECISION STOKES(3,MAXCHANS),STOKESQ(3,MAXCHANS)
      DOUBLE PRECISION STOKESC(3,MAXCHANS)
      DOUBLE PRECISION CSTOKES(3,MAXCHANS,MAXCYCLES)
      INTEGER NCYC

      COMMON /POL_CUMULATIVE/STOKES,STOKESQ,STOKESC,CSTOKES,NCYC
      
