INTERFACE TDERIV

  PARAMETER INPUT
    TYPE 'NDF'
    POSITION 1
    PPATH 'CURRENT'
    VPATH 'PROMPT'
    PROMPT 'Input Dataset'
  ENDPARAMETER

  PARAMETER NPOINTS
    TYPE '_INTEGER'
    POSITION 2
    PPATH 'CURRENT,DEFAULT'
    VPATH 'PROMPT'
    PROMPT 'Number of points to use for slope'
  ENDPARAMETER

  PARAMETER OUTPUT
    TYPE 'NDF'   
    POSITION 3
    PPATH 'CURRENT'
    VPATH 'PROMPT'
    PROMPT 'Output Dataset'
  ENDPARAMETER

ENDINTERFACE
