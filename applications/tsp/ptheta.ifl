INTERFACE PTHETA

   PARAMETER INPUT
      TYPE 'ENDIF'   
      POSITION 1
      VPATH 'PROMPT'
      PPATH 'CURRENT'
      PROMPT 'Polarization Spectrum'
   ENDPARAMETER

  PARAMETER LSTART
    TYPE '_REAL'
    VPATH 'PROMPT'
    PPATH 'CURRENT,DYNAMIC'
    PROMPT 'First Wavelength to Use'
  ENDPARAMETER

  PARAMETER LEND
    TYPE '_REAL'
    VPATH 'PROMPT'
    PPATH 'CURRENT,DYNAMIC'
    PROMPT 'Last Wavelength to Use'
  ENDPARAMETER

ENDINTERFACE
