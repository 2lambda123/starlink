INTERFACE LHATPOL

  PARAMETER FIGARO
    TYPE '_CHAR'
    VPATH 'PROMPT'
    PPATH 'CURRENT'
    POSITION 1
    PROMPT 'Hatfield FIGARO format Input file '
  ENDPARAMETER

  PARAMETER FILE
     TYPE 'FILE'
     VPATH 'PROMPT'
     PPATH 'CURRENT,DEFAULT'
     DEFAULT HATPOL.LIS
     PROMPT 'Listing File'
  ENDPARAMETER                   

ENDINTERFACE
