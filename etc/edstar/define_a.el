(new-token "BAD"
	   "STATUS .NE. SAI__OK"
	   '((alias . t)))

(new-token "CLOC"
	   "CHARACTER * ( DAT__SZLOC )"
	   '((alias . t)))

(new-token "CMOD"
	   "CHARACTER * ( DAT__SZMOD )"
	   '((alias . t)))

(new-token "CNAM"
	   "CHARACTER * ( DAT__SZNAM )"
	   '((alias . t)))

(new-token "CTYP"
	   "CHARACTER * ( DAT__SZTYP )"
	   '((alias . t)))

(new-token "OK"
	   "STATUS .EQ. SAI__OK"
	   '((alias . t)))

(new-token "ST"
	   "STATUS"
	   '((alias . t)))
