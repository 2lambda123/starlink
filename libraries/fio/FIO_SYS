*  Global Constants:
      INTEGER FIO__BASE          ! Base address for package
      PARAMETER ( FIO__BASE = 0 )
      INTEGER FIO__MXFIL         ! Maximum number of files
      PARAMETER ( FIO__MXFIL = 40 )
      INTEGER FIO__MXUNT         ! Maximum number of units
      PARAMETER ( FIO__MXUNT = 40 )
      INTEGER FIO__BSUNT         ! Base Fortran unit number 
      PARAMETER ( FIO__BSUNT = 40 )
