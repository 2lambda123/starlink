#!/bin/csh
set ff = " "
foreach n (atl_*.f)
   if( $n != "atl_test.f" ) then
      set ff = "$ff $n"
   endif
end
rm -f atl_routines.tex sun257.tex
$STARCONF_DEFAULT_STARLINK/bin/sst/prolat \"$ff\" out=atl_routines.tex \
                               noatask nodocument single nopage
cat sun_head.tex atl_routines.tex sun_tail.tex > sun257.tex

