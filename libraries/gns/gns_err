*  Construct the error numbers according to SUN/104
*  CODE = 134250498 + 65536 * <fac> + 8 * <mes>
*  The GNS facilty number = 1504
      INTEGER IBASE
      PARAMETER (IBASE=232816642)

*  Invalid workstation identifier
      INTEGER GNS__INWKID
      PARAMETER (GNS__INWKID = 8 + IBASE)

*  Workstation type not defined in database
      INTEGER GNS__WKNDEF
      PARAMETER (GNS__WKNDEF = 16 + IBASE)

*  Not an interactive job
      INTEGER GNS__NOTINT
      PARAMETER (GNS__NOTINT = 24 + IBASE)

*  Device name not recognized
      INTEGER GNS__NAMNR
      PARAMETER (GNS__NAMNR = 32 + IBASE)

*  Package not supported
      INTEGER GNS__PKGNS
      PARAMETER (GNS__PKGNS = 40 + IBASE)

*   Database file has an invalid format
      INTEGER  GNS__DBINV
      PARAMETER (GNS__DBINV = 48 + IBASE)

*   Database file contains too many records
      INTEGER  GNS__DBOVF
      PARAMETER (GNS__DBOVF = 56 + IBASE)

*   Unable to open database file
      INTEGER  GNS__DBOPE
      PARAMETER (GNS__DBOPE = 64 + IBASE)

*   Error reading database file
      INTEGER GNS__DBRDE
      PARAMETER (GNS__DBRDE = 72 + IBASE)

*   Database format error
      INTEGER GNS__DBFME
      PARAMETER (GNS__DBFME = 80 + IBASE)

*   Unknown characteristic
      INTEGER GNS__UNKCHA
      PARAMETER (GNS__UNKCHA = 88 + IBASE)

*   Ambiguous workstation name
      INTEGER GNS__AMBNAM
      PARAMETER (GNS__AMBNAM = 96 + IBASE)

*   Description file version not supported
      INTEGER GNS__VNSUP
      PARAMETER (GNS__VNSUP = 104 + IBASE)

*   AGI type not defined
      INTEGER GNS__AGTND
      PARAMETER (GNS__AGTND = 112 + IBASE)

*   AGI name not recognised
      INTEGER GNS__AGNNR
      PARAMETER (GNS__AGNNR = 120 + IBASE)

*   No more connection identifiers can be assigned
      INTEGER GNS__CIOVF
      PARAMETER (GNS__CIOVF = 128 + IBASE)

*   No overlay on the device
      INTEGER GNS__NOVER
      PARAMETER (GNS__NOVER = 136 + IBASE)

