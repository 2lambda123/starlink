    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>M J Bly</name>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>1990,1993,1996,1998</year>
    <name>CLRC</name>
  </copyright>
</component>
