      <name>A R Wood</name>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>1991,1992</year>
    <name>CLRC</name>
  </copyright>
</component>
