<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@">
  <version>@PACKAGE_VERSION@</version>
  <path>docs/sun/201</path>
  <description>LATEX2HTML LaTeX-to-HTML converter</description>
  <abstract><p>
     LATEX2HTML converts \LaTeX\ source files into a set of HTML
     files, suitable for browsing on the World Wide Web. It is used by
     STAR2HTML, which is the preferred program to use for converting
     Starlink documents to hypertext form.  
    </p></abstract>  
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>M.J. Bly</name>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>1995</year>
    <name>CLRC</name>
  </copyright>
</component>
