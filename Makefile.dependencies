	            >configure-output.log; \
	    elif test ! -f Makefile; then \
	        { t="Directory unconfigured but BUILDSUPPORT_PREFIX undefined";\
	          echo $$t >configure-output.log; echo $$t >&2; \
	          exit 1; }; \
	    else \
	        echo "No configuration necessary" >configure-output.log; \
	    fi \
	  && $(MAKE_CPT)

