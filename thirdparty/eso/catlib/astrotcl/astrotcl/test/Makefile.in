CC           	= @CC@
CXX           	= @CXX@
CFLAGS  	= @CXXDEBUGFLAGS@
TOPDIR      	= @DESTDIR@
LIBDIR      	= @LIBDIR@
DEPEND_TARGET	= @DEPEND_TARGET@

SRCS 		=  

OBJS 		=  

PROGS 		=  

INCLUDES	= 

CC_SWITCHES	= $(CFLAGS) $(DEFINES) $(INCLUDES)

LIBRARIES       = 

DEPEND_FLAGS    = $(CC_SWITCHES) @CXX_INCLUDE@ @CXX_DEPEND_FLAGS@
SHELL    	= /bin/sh
AR       	= ar rc
RM       	= rm -f

all: 

test: $(PROGS)

All:
	cd ../..; $(MAKE); $(MAKE)

install:

clean: 
	$(RM) $(PROGS) $(OBJS) *\~ "#"* *.o

distclean: clean 
	rm -f Makefile depclean

.SUFFIXES: .C
.c.o:
	$(CC) -c $(CC_SWITCHES) $<

.C.o:
	$(CXX) -c $(CC_SWITCHES) $<

man:

include @SRC_ROOT@/makefile.mk
include ../../depend.mk
